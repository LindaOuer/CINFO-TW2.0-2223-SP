<ul>
  {% for st in list %}
    <li>Les matchs qui se déroulent au stade
      <b>{{st.nom}}
      </b>
      situé à
      <b>{{st.ville}}</b>
      sont comme suit
      <hr>
      <table border="1">
        <tr>
          <th>
            Numéro Match
          </th>
          <th>
            date
          </th>
          <th>
            nombre
          </th>
        </tr>
        {% for m in st.matches %}
          <tr>
            <td>{{ m.id}}
            </td>
            <td>{{ m.dateM|date("m/d/Y")}}
            </td>
            <td>{{ m.nbSpectateurs}}
            </td>
          </tr>
        {% endfor %}
      </table>

    </li>
    <br>
  {% endfor %}
</ul>
